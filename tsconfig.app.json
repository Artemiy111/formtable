{
  "extends": "@vue/tsconfig/tsconfig.dom.json",
 "include": [
    "src/**/*.ts", 
    "src/**/*.tsx",
    "src/**/*.vue",
    "auto-imports.d.ts",
    "components.d.ts"
  ],
  "compilerOptions": {
    "tsBuildInfoFile": "./node_modules/.tmp/tsconfig.app.tsbuildinfo",
    "paths": {
      "@/*": [
        "./src/*"
      ],
       "#build/ui": [
        "./node_modules/.nuxt-ui/ui"
      ]
    }
  }
}